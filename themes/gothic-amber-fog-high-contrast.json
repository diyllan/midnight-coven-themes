  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#776655",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "constant.other.symbol"
      ],
      "settings": {
        "foreground": "#ffcc55"
      }
    },
    {
      "name": "Constants and Numbers",
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.character",
        "constant.escape"
      ],
      "settings": {
        "foreground": "#ffaa44"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "string constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#ffb020",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Functions and Classes",
      "scope": [
        "entity.name.function",
        "entity.name.class",
        "entity.name.type",
        "entity.name.namespace",
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#aaee66",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Storage types",
      "scope": [
        "storage.type"
      ],
      "settings": {
        "foreground": "#ee77cc",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Types and Support",
      "scope": [
        "support.class",
        "support.type",
        "support.function",
        "support.constant"
      ],
      "settings": {
        "foreground": "#5588cc"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#44ccaa"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "meta.brace",
        "meta.delimiter"
      ],
      "settings": {
        "foreground": "#ddccbb"
      }
    },
    {
      "name": "JSON Property Names",
      "scope": [
        "support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#ffb020",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Headings",
      "scope": [
        "markup.heading"
      ],
      "settings": {
        "foreground": "#ffb020",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Bold",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup Links",
      "scope": [
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#5588cc"
      }
    },
    {
      "name": "Markup Lists",
      "scope": [
        "markup.list"
      ],
      "settings": {
        "foreground": "#ffcc55"
      }
    },
    {
      "name": "Markup Quotes",
      "scope": [
        "markup.quote"
      ],
      "settings": {
        "foreground": "#ee77cc",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Inline Documentation",
      "scope": [
        "comment.block.documentation"
      ],
      "settings": {
        "foreground": "#887766",
        "fontStyle": "italic"
      }
    },
    {
      "name": "RegExp",
      "scope": [
        "string.regexp"
      ],
      "settings": {
        "foreground": "#44ccaa"
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "tag.decorator.js",
        "meta.tag.decorator",
        "meta.decorator"
      ],
      "settings": {
        "foreground": "#ee77cc",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Template Literals",
      "scope": [
        "string.template",
        "punctuation.definition.string.template"
      ],
      "settings": {
        "foreground": "#ffcc55"
      }
    },
    {
      "name": "Special Variables",
      "scope": [
        "variable.language",
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#ff7766",
        "fontStyle": "italic"
      }
    }
  ]
}
