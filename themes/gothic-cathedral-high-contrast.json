  "tokenColors": [
    {
      "name": "JSON Property Names",
      "scope": [
        "support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#9a7286",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#8a6a71"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "constant.other.symbol"
      ],
      "settings": {
        "foreground": "#daa671"
      }
    },
    {
      "name": "Constants and Numbers",
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.character",
        "constant.escape"
      ],
      "settings": {
        "foreground": "#da8971"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "variable.parameter",
        "variable.other"
      ],
      "settings": {
        "foreground": "#f0e8df"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#c47bab",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function"
      ],
      "settings": {
        "foreground": "#e97b97"
      }
    },
    {
      "name": "Classes and Types",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "entity.other.inherited-class",
        "support.class",
        "support.type"
      ],
      "settings": {
        "foreground": "#a8cc7b",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Storage types",
      "scope": [
        "storage.type"
      ],
      "settings": {
        "foreground": "#7babe9",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Tags and HTML Elements",
      "scope": [
        "entity.name.tag",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#7babe9"
      }
    },
    {
      "name": "Attributes",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#a8cc7b"
      }
    },
    {
      "name": "Property",
      "scope": [
        "variable.other.property",
        "support.variable.property"
      ],
      "settings": {
        "foreground": "#e97b97"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#da8971"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#ff5a5a"
      }
    },
    {
      "name": "Modified",
      "scope": [
        "markup.changed",
        "markup.deleted",
        "markup.inserted"
      ],
      "settings": {
        "foreground": "#e97b97"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "punctuation.definition",
        "punctuation.separator",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Markdown Headings",
      "scope": [
        "markup.heading",
        "entity.name.section"
      ],
      "settings": {
        "foreground": "#daa671",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Link",
      "scope": [
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#7babe9"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "foreground": "#c47bab",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Inline Documentation",
      "scope": [
        "comment.block.documentation"
      ],
      "settings": {
        "foreground": "#a07889",
        "fontStyle": "italic"
      }
    },
    {
      "name": "RegExp",
      "scope": [
        "string.regexp"
      ],
      "settings": {
        "foreground": "#5a8bcc"
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "tag.decorator.js",
        "meta.tag.decorator",
        "meta.decorator"
      ],
      "settings": {
        "foreground": "#c47bab",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Template Literals",
      "scope": [
        "string.template",
        "punctuation.definition.string.template"
      ],
      "settings": {
        "foreground": "#daa671"
      }
    },
    {
      "name": "Special Variables",
      "scope": [
        "variable.language",
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#e97b97",
        "fontStyle": "italic bold"
      }
    }
  ]
}
