    "statusBar.debuggingBackground": "#50ff50",
    "statusBar.debuggingForeground": "#000000",
    "statusBar.noFolderBackground": "#102010",
    "statusBarItem.remoteBackground": "#50ff50",
    "statusBarItem.remoteForeground": "#000000",
    "titleBar.activeBackground": "#030803",
    "titleBar.activeForeground": "#ffffff",
    "titleBar.inactiveBackground": "#050a05",
    "titleBar.inactiveForeground": "#5a8a5a",
    "titleBar.border": "#102010",
    "menu.background": "#030803",
    "menu.foreground": "#e0ffe0",
    "menu.selectionBackground": "#203a20",
    "menu.selectionForeground": "#ffffff",
    "menubar.selectionBackground": "#203a20",
    "menubar.selectionForeground": "#ffffff",
    "list.activeSelectionBackground": "#203a20",
    "list.activeSelectionForeground": "#ffffff",
    "list.inactiveSelectionBackground": "#102010",
    "list.inactiveSelectionForeground": "#e0ffe0",
    "list.hoverBackground": "#102010",
    "list.hoverForeground": "#ffffff",
    "list.highlightForeground": "#50ff50",
    "list.errorForeground": "#ff5050",
    "list.warningForeground": "#ffaa50",
    "tab.activeBackground": "#102010",
    "tab.activeForeground": "#ffffff",
    "tab.inactiveBackground": "#081008",
    "tab.inactiveForeground": "#70c070",
    "tab.activeBorderTop": "#50ff50",
    "tab.unfocusedActiveBorderTop": "#40a040",
    "editorGroupHeader.tabsBackground": "#050a05",
    "terminal.ansiBlack": "#050a05",
    "terminal.ansiRed": "#ff5050",
    "terminal.ansiGreen": "#50ff50",
    "terminal.ansiYellow": "#ffaa50",
    "terminal.ansiBlue": "#5080ff",
    "terminal.ansiMagenta": "#aa50aa",
    "terminal.ansiCyan": "#50d0d0",
    "terminal.ansiWhite": "#e0ffe0",
    "terminal.ansiBrightBlack": "#5a8a5a",
    "terminal.ansiBrightRed": "#ff7070",
    "terminal.ansiBrightGreen": "#70ff70",
    "terminal.ansiBrightYellow": "#ffcc70",
    "terminal.ansiBrightBlue": "#70a0ff",
    "terminal.ansiBrightMagenta": "#cc70cc",
    "terminal.ansiBrightCyan": "#70f0f0",
    "terminal.ansiBrightWhite": "#ffffff",
    "terminal.background": "#050a05",
    "terminal.foreground": "#e0ffe0",
    "terminalCursor.background": "#050a05",
    "terminalCursor.foreground": "#50ff50",
    "editor.findMatchBackground": "#203a2080",
    "editor.findMatchHighlightBackground": "#203a2060",
    "editor.findRangeHighlightBackground": "#203a2040",
    "editor.wordHighlightBackground": "#203a2040",
    "editor.wordHighlightStrongBackground": "#203a2080",
    "editor.hoverHighlightBackground": "#203a2060",
    "editorLink.activeForeground": "#50ff50",
    "editorBracketMatch.background": "#203a2060",
    "editorBracketMatch.border": "#50ff50",
    "editorError.foreground": "#ff5050",
    "editorWarning.foreground": "#ffaa50",
    "editorInfo.foreground": "#5080ff",
    "editorHint.foreground": "#50ff50",
    "editorIndentGuide.background": "#10201060",
    "editorIndentGuide.activeBackground": "#203a20",
    "editorOverviewRuler.findMatchForeground": "#50ff50",
    "editorOverviewRuler.modifiedForeground": "#ffaa50",
    "editorOverviewRuler.deletedForeground": "#ff5050",
    "editorOverviewRuler.errorForeground": "#ff5050",
    "editorOverviewRuler.warningForeground": "#ffaa50",
    "editorOverviewRuler.bracketMatchForeground": "#5080ff",
    "button.background": "#50ff50",
    "button.foreground": "#000000",
    "button.hoverBackground": "#70ff70",
    "dropdown.background": "#030803",
    "dropdown.foreground": "#e0ffe0",
    "dropdown.border": "#102010",
    "focusBorder": "#50ff50",
    "inputOption.activeBorder": "#50ff50",
    "input.background": "#030803",
    "input.foreground": "#e0ffe0",
    "input.border": "#102010",
    "input.placeholderForeground": "#5a8a5a",
    "scrollbarSlider.background": "#203a2040",
    "scrollbarSlider.hoverBackground": "#203a2080",
    "scrollbarSlider.activeBackground": "#50ff5080",
    "badge.background": "#50ff50",
    "badge.foreground": "#000000",
    "progressBar.background": "#50ff50"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#5a8a5a",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "constant.other.symbol"
      ],
      "settings": {
        "foreground": "#ffaa50"
      }
    },
    {
      "name": "Constants and Numbers",
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.character",
        "constant.escape"
      ],
      "settings": {
        "foreground": "#ffcc70"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "string constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#e0ffe0"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#50ff50",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Functions and Classes",
      "scope": [
        "entity.name.function",
        "entity.name.class",
        "entity.name.type",
        "entity.name.namespace",
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#70ff70",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Storage types",
      "scope": [
        "storage.type"
      ],
      "settings": {
        "foreground": "#aa50aa",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Types and Support",
      "scope": [
        "support.class",
        "support.type",
        "support.function",
        "support.constant"
      ],
      "settings": {
        "foreground": "#5080ff"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#50ff50"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "meta.brace",
        "meta.delimiter"
      ],
      "settings": {
        "foreground": "#70c070"
      }
    },
    {
      "name": "JSON Property Names",
      "scope": [
        "support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#50ff50",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Headings",
      "scope": [
        "markup.heading"
      ],
      "settings": {
        "foreground": "#50ff50",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Bold",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup Links",
      "scope": [
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#5080ff"
      }
    },
    {
      "name": "Markup Lists",
      "scope": [
        "markup.list"
      ],
      "settings": {
        "foreground": "#ffaa50"
      }
    },
    {
      "name": "Markup Quotes",
      "scope": [
        "markup.quote"
      ],
      "settings": {
        "foreground": "#aa50aa",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Inline Documentation",
      "scope": [
        "comment.block.documentation"
      ],
      "settings": {
        "foreground": "#6a9a6a",
        "fontStyle": "italic"
      }
    },
    {
      "name": "RegExp",
      "scope": [
        "string.regexp"
      ],
      "settings": {
        "foreground": "#50d0d0"
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "tag.decorator.js",
        "meta.tag.decorator",
        "meta.decorator"
      ],
      "settings": {
        "foreground": "#aa50aa",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Template Literals",
      "scope": [
        "string.template",
        "punctuation.definition.string.template"
      ],
      "settings": {
        "foreground": "#ffaa50"
      }
    },
    {
      "name": "Special Variables",
      "scope": [
        "variable.language",
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#ff5050",
        "fontStyle": "italic"
      }
    }
  ]
}
