  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#6a5a7a",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "constant.other.symbol"
      ],
      "settings": {
        "foreground": "#d0a0ff"
      }
    },
    {
      "name": "Constants and Numbers",
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.character",
        "constant.escape"
      ],
      "settings": {
        "foreground": "#e0b0ff"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "string constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#e0d8f0"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#ff90b0",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Functions and Classes",
      "scope": [
        "entity.name.function",
        "entity.name.class",
        "entity.name.type",
        "entity.name.namespace",
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#ff70a0",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Storage types",
      "scope": [
        "storage.type"
      ],
      "settings": {
        "foreground": "#7080ff",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Types and Support",
      "scope": [
        "support.class",
        "support.type",
        "support.function",
        "support.constant"
      ],
      "settings": {
        "foreground": "#70a0ff"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#70d0ff"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "meta.brace",
        "meta.delimiter"
      ],
      "settings": {
        "foreground": "#a090c0"
      }
    },
    {
      "name": "JSON Property Names",
      "scope": [
        "support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#ff90b0",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Headings",
      "scope": [
        "markup.heading"
      ],
      "settings": {
        "foreground": "#ff70a0",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Bold",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup Links",
      "scope": [
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#70a0ff"
      }
    },
    {
      "name": "Markup Lists",
      "scope": [
        "markup.list"
      ],
      "settings": {
        "foreground": "#d0a0ff"
      }
    },
    {
      "name": "Markup Quotes",
      "scope": [
        "markup.quote"
      ],
      "settings": {
        "foreground": "#7080ff",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Inline Documentation",
      "scope": [
        "comment.block.documentation"
      ],
      "settings": {
        "foreground": "#7a6a8a",
        "fontStyle": "italic"
      }
    },
    {
      "name": "RegExp",
      "scope": [
        "string.regexp"
      ],
      "settings": {
        "foreground": "#70d0ff"
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "tag.decorator.js",
        "meta.tag.decorator",
        "meta.decorator"
      ],
      "settings": {
        "foreground": "#7080ff",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Template Literals",
      "scope": [
        "string.template",
        "punctuation.definition.string.template"
      ],
      "settings": {
        "foreground": "#d0a0ff"
      }
    },
    {
      "name": "Special Variables",
      "scope": [
        "variable.language",
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#ff5070",
        "fontStyle": "italic"
      }
    }
  ]
}
