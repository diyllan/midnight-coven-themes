{
  "name": "Gothic Cozy",
  "type": "dark",
  "semanticHighlighting": true,
  "colors": {
    "editor.background": "#2a2025",
    "editor.foreground": "#ffffff",
    "editor.lineHighlightBackground": "#38302a",
    "editor.selectionBackground": "#483832",
    "editorCursor.foreground": "#d0a080",
    "activityBar.background": "#352a24",
    "activityBar.foreground": "#ffffff",
    "activityBarBadge.background": "#d0a080",
    "activityBarBadge.foreground": "#2a2025",
    "sideBar.background": "#2d2420",
    "sideBar.foreground": "#ffffff",
    "sideBarTitle.foreground": "#d0a080",
    "sideBarSectionHeader.background": "#352a24",
    "sideBarSectionHeader.foreground": "#ffffff",
    "list.activeSelectionBackground": "#483832",
    "list.activeSelectionForeground": "#ffffff",
    "list.inactiveSelectionBackground": "#38302a",
    "list.inactiveSelectionForeground": "#ffffff",
    "list.hoverBackground": "#352a24",
    "list.hoverForeground": "#ffffff",
    "list.highlightForeground": "#d0a080",
    "statusBar.background": "#352a24",
    "statusBar.foreground": "#ffffff",
    "statusBar.debuggingBackground": "#d0a080",
    "statusBar.debuggingForeground": "#2a2025",
    "statusBar.noFolderBackground": "#352a24",
    "statusBarItem.remoteBackground": "#d0a080",
    "statusBarItem.remoteForeground": "#2a2025",
    "titleBar.activeBackground": "#352a24",
    "titleBar.activeForeground": "#d0c0b0",
    "titleBar.inactiveBackground": "#2d2420",
    "titleBar.inactiveForeground": "#a09080",
    "menubar.selectionBackground": "#483832",
    "menubar.selectionForeground": "#e0d8d0",
    "menu.background": "#2d2420",
    "menu.foreground": "#d0c0b0",
    "menu.selectionBackground": "#483832",
    "menu.selectionForeground": "#e0d8d0",
    "menu.separatorBackground": "#352a24",
    "tab.activeBackground": "#2a2025",
    "tab.activeForeground": "#d0c0b0",
    "tab.inactiveBackground": "#2d2420",
    "tab.inactiveForeground": "#a09080",
    "tab.activeBorderTop": "#d0a080",
    "tab.unfocusedActiveBorderTop": "#a08060",
    "editorGroupHeader.tabsBackground": "#352a24",
    "terminal.ansiBlack": "#2a2025",
    "terminal.ansiRed": "#c07060",
    "terminal.ansiGreen": "#80a070",
    "terminal.ansiYellow": "#d0a080",
    "terminal.ansiBlue": "#7090c0",
    "terminal.ansiMagenta": "#b08090",
    "terminal.ansiCyan": "#60a0a0",
    "terminal.ansiWhite": "#d0c0b0",
    "terminal.ansiBrightBlack": "#5a5055",
    "terminal.ansiBrightRed": "#e09080",
    "terminal.ansiBrightGreen": "#a0c090",
    "terminal.ansiBrightYellow": "#e0b090",
    "terminal.ansiBrightBlue": "#90b0e0",
    "terminal.ansiBrightMagenta": "#d0a0b0",
    "terminal.ansiBrightCyan": "#80c0c0",
    "terminal.ansiBrightWhite": "#e0d8d0",
    "editorIndentGuide.background": "#38302a70",
    "editorIndentGuide.activeBackground": "#483832",
    "editorOverviewRuler.findMatchForeground": "#d0a080",
    "editorOverviewRuler.modifiedForeground": "#d0a080",
    "editorOverviewRuler.deletedForeground": "#c07060",
    "editorOverviewRuler.errorForeground": "#e09080",
    "editorOverviewRuler.warningForeground": "#d0a080",
    "editorOverviewRuler.bracketMatchForeground": "#7090c0",
    "diffEditor.insertedTextBackground": "#80a07020",
    "diffEditor.removedTextBackground": "#c0706020",
    "merge.currentHeaderBackground": "#d0a08040",
    "merge.currentContentBackground": "#d0a08020",
    "merge.incomingHeaderBackground": "#7090c040",
    "merge.incomingContentBackground": "#7090c020",
    "editor.findMatchBackground": "#d0a08060",
    "editor.findMatchHighlightBackground": "#d0a08040",
    "editor.wordHighlightBackground": "#d0a08030",
    "editor.wordHighlightStrongBackground": "#d0a08050",
    "editor.stackFrameHighlightBackground": "#d0a08040",
    "editor.focusedStackFrameHighlightBackground": "#d0a08060",
    "inputValidation.errorBackground": "#c07060",
    "inputValidation.errorBorder": "#e09080",
    "inputValidation.infoBackground": "#7090c0",
    "inputValidation.infoBorder": "#90b0e0",
    "inputValidation.warningBackground": "#d0a080",
    "inputValidation.warningBorder": "#e0b090"
  },
  "tokenColors": [
    {
      "name": "JSON Property Names",
      "scope": [
        "support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#d0a080",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#a09080"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "constant.other.symbol"
      ],
      "settings": {
        "foreground": "#e09080"
      }
    },
    {
      "name": "Constants and Numbers",
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.character",
        "constant.escape"
      ],
      "settings": {
        "foreground": "#d0a080"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "variable.parameter",
        "variable.other"
      ],
      "settings": {
        "foreground": "#d0c0b0"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#b08090",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function"
      ],
      "settings": {
        "foreground": "#7090c0"
      }
    },
    {
      "name": "Classes and Types",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "entity.other.inherited-class",
        "support.class",
        "support.type"
      ],
      "settings": {
        "foreground": "#80a070",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Storage types",
      "scope": [
        "storage.type"
      ],
      "settings": {
        "foreground": "#b08090",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Tags and HTML Elements",
      "scope": [
        "entity.name.tag",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#7090c0"
      }
    },
    {
      "name": "Attributes",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#80a070"
      }
    },
    {
      "name": "Property",
      "scope": [
        "variable.other.property",
        "support.variable.property"
      ],
      "settings": {
        "foreground": "#e09080"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#d0a080"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#e09080"
      }
    },
    {
      "name": "Modified",
      "scope": [
        "markup.changed",
        "markup.deleted",
        "markup.inserted"
      ],
      "settings": {
        "foreground": "#d0a080"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "punctuation.definition",
        "punctuation.separator",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#a09080"
      }
    },
    {
      "name": "Markdown Headings",
      "scope": [
        "markup.heading",
        "entity.name.section"
      ],
      "settings": {
        "foreground": "#d0a080",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Link",
      "scope": [
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#7090c0"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "foreground": "#d0a080",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Inline Documentation",
      "scope": [
        "comment.block.documentation"
      ],
      "settings": {
        "foreground": "#b0a090",
        "fontStyle": "italic"
      }
    },
    {
      "name": "RegExp",
      "scope": [
        "string.regexp"
      ],
      "settings": {
        "foreground": "#60a0a0"
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "tag.decorator.js",
        "meta.tag.decorator",
        "meta.decorator"
      ],
      "settings": {
        "foreground": "#b08090",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Template Literals",
      "scope": [
        "string.template",
        "punctuation.definition.string.template"
      ],
      "settings": {
        "foreground": "#d0a080"
      }
    },
    {
      "name": "Special Variables",
      "scope": [
        "variable.language",
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#c07060",
        "fontStyle": "italic"
      }
    }
  ]
}
