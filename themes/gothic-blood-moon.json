{
  "name": "Gothic Blood Moon",
  "type": "dark",
  "semanticHighlighting": true,
  "themeSpecificSettings": {
    "cursorTrailEffect": "eclipse",
    "focusEffects": "lunar",
    "animationIntensity": "high",
    "specialElements": "crimson"
  },
  "colors": {
