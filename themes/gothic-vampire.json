  "semanticTokenColors": {
    "property": "#db929e",
    "type": {
      "foreground": "#a070b0",
      "bold": true
    },
    "class": {
      "foreground": "#cf4070",
      "bold": true
    },
    "enum": {
      "foreground": "#ff9070",
      "bold": true
    },
    "interface": {
      "foreground": "#a070b0",
      "italic": true
    },
    "function": "#cf4070",
    "variable.readonly": {
      "foreground": "#d060a0",
      "italic": true
    },
    "macro": {
      "foreground": "#ff5077",
      "bold": true
    },
    "method": {
      "foreground": "#cf4070",
      "bold": true
    },
    "namespace": {
      "foreground": "#d060a0",
      "italic": true
    },
    "parameter": {
      "foreground": "#f0d8e0",
      "italic": true
    },
    "property.readonly": {
      "foreground": "#ff7090",
      "italic": true
    },
    "constructor": {
      "foreground": "#ff5077",
      "bold": true
    },
    "parameter.declaration": {
      "foreground": "#f0d8e0",
      "underline": true
    }
  },
  "tokenColors": [
    {
      "name": "JSON Property Names",
      "scope": [
        "support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#db929e",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#907080"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "constant.other.symbol"
      ],
      "settings": {
        "foreground": "#ff9070"
      }
    },
    {
      "name": "Constants and Numbers",
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.character",
        "constant.escape"
      ],
      "settings": {
        "foreground": "#d060a0"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "string constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#f0d8e0"
      }
    },
    {
      "name": "Inline Documentation",
      "scope": [
        "comment.block.documentation"
      ],
      "settings": {
        "foreground": "#a08090",
        "fontStyle": "italic"
      }
    },
    {
      "name": "RegExp",
      "scope": [
        "string.regexp"
      ],
      "settings": {
        "foreground": "#a070b0"
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "tag.decorator.js",
        "meta.tag.decorator",
        "meta.decorator"
      ],
      "settings": {
        "foreground": "#d060a0",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#ff5077",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Functions and Classes",
      "scope": [
        "entity.name.function",
        "entity.name.class",
        "entity.name.type",
        "entity.name.namespace",
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#cf4070",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Storage types",
      "scope": [
        "storage.type"
      ],
      "settings": {
        "foreground": "#a03060",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Types and Support",
      "scope": [
        "support.class",
        "support.type",
        "support.function",
        "support.constant"
      ],
      "settings": {
        "foreground": "#a070b0"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#ff7090"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "meta.brace",
        "meta.delimiter"
      ],
      "settings": {
        "foreground": "#cf4070"
      }
    },
    {
      "name": "Markup Headings",
      "scope": [
        "markup.heading"
      ],
      "settings": {
        "foreground": "#ff5077",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Bold",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup Links",
      "scope": [
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#a070b0"
      }
    },
    {
      "name": "Markup Lists",
      "scope": [
        "markup.list"
      ],
      "settings": {
        "foreground": "#ff9070"
      }
    },
    {
      "name": "Markup Quotes",
      "scope": [
        "markup.quote"
      ],
      "settings": {
        "foreground": "#d060a0",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Special Vampiric SQL Highlights",
      "scope": [
        "keyword.other.DML.sql",
        "support.function.aggregate.sql"
      ],
      "settings": {
        "foreground": "#ff5077",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Blood Magic Variables",
      "scope": [
        "variable.other.property",
        "variable.language.special"
      ],
      "settings": {
        "foreground": "#ff7090",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Nocturnal Imports",
      "scope": [
        "keyword.control.import",
        "keyword.control.export",
        "keyword.control.from"
      ],
      "settings": {
        "foreground": "#cf4070",
        "fontStyle": "bold italic"
      }
    },
    {
      "name": "Bloodline Inheritance",
      "scope": [
        "storage.modifier.inheritance",
        "storage.modifier.extends"
      ],
      "settings": {
        "foreground": "#ff5077",
        "fontStyle": "bold underline"
      }
    }
  ]
}
