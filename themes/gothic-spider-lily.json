    "tokenColors": [
        {
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#4d1515"
            }
        },
        {
            "scope": [
                "constant",
                "entity.name.constant",
                "variable.other.constant",
                "variable.language"
            ],
            "settings": {
                "foreground": "#ff6666"
            }
        },
        {
            "scope": [
                "entity",
                "entity.name"
            ],
            "settings": {
                "foreground": "#ff8888"
            }
        },
        {
            "scope": "variable.parameter.function",
            "settings": {
                "foreground": "#dc143c"
            }
        },
        {
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#8b0000"
            }
        },
        {
            "scope": "keyword",
            "settings": {
                "foreground": "#8b0000",
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "storage",
                "storage.type"
            ],
            "settings": {
                "foreground": "#8b0000",
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "storage.modifier.package",
                "storage.modifier.import",
                "storage.type.java"
            ],
            "settings": {
                "foreground": "#dc143c"
            }
        },
        {
            "scope": [
                "string",
                "punctuation.definition.string",
                "string punctuation.section.embedded source"
            ],
            "settings": {
                "foreground": "#b22222"
            }
        },
        {
            "scope": "support",
            "settings": {
                "foreground": "#ff6666"
            }
        },
        {
            "scope": "meta.property-name",
            "settings": {
                "foreground": "#ff8888"
            }
        },
        {
            "scope": "variable",
            "settings": {
                "foreground": "#dc143c"
            }
        },
        {
            "scope": "variable.other",
            "settings": {
                "foreground": "#dc143c"
            }
        },
        {
            "scope": "invalid.broken",
            "settings": {
                "fontStyle": "bold italic underline",
                "foreground": "#ff6666"
            }
        },
        {
            "scope": "invalid.deprecated",
            "settings": {
                "fontStyle": "bold italic underline",
                "foreground": "#ff6666"
            }
        },
        {
            "scope": "invalid.illegal",
            "settings": {
                "fontStyle": "italic underline",
                "foreground": "#ff6666"
            }
        },
        {
            "scope": "invalid.unimplemented",
            "settings": {
                "fontStyle": "bold italic underline",
                "foreground": "#ff6666"
            }
        },
        {
            "scope": "carriage-return",
            "settings": {
                "fontStyle": "italic underline",
                "foreground": "#ff6666"
            }
        },
        {
            "scope": "message.error",
            "settings": {
                "foreground": "#ff6666"
            }
        },
        {
            "scope": "string source",
            "settings": {
                "foreground": "#dc143c"
            }
        },
        {
            "scope": "string variable",
            "settings": {
                "foreground": "#ff6666"
            }
        },
        {
            "scope": [
                "source.regexp",
                "string.regexp"
            ],
            "settings": {
                "foreground": "#b22222"
            }
        },
        {
            "scope": [
                "string.regexp.character-class",
                "string.regexp constant.character.escape",
                "string.regexp source.ruby.embedded",
                "string.regexp string.regexp.arbitrary-repitition"
            ],
            "settings": {
                "foreground": "#b22222"
            }
        },
        {
            "scope": "string.regexp constant.character.escape",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#8b0000"
            }
        },
        {
            "scope": "support.constant",
            "settings": {
                "foreground": "#ff6666"
            }
        },
        {
            "scope": "support.variable",
            "settings": {
                "foreground": "#ff6666"
            }
        },
        {
            "scope": "meta.module-reference",
            "settings": {
                "foreground": "#ff6666"
            }
        },
        {
            "scope": "punctuation.definition.list.begin.markdown",
            "settings": {
                "foreground": "#ff8888"
            }
        },
        {
            "scope": [
                "markup.heading",
                "markup.heading entity.name"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#ff6666"
            }
        },
        {
            "scope": "markup.quote",
            "settings": {
                "foreground": "#8b0000"
            }
        },
        {
            "scope": "markup.italic",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#dc143c"
            }
        },
        {
            "scope": "markup.bold",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#dc143c"
            }
        },
        {
            "scope": "markup.raw",
            "settings": {
                "foreground": "#ff6666"
            }
        },
        {
            "scope": [
                "markup.deleted",
                "meta.diff.header.from-file",
                "punctuation.definition.deleted"
            ],
            "settings": {
                "background": "#ff666620",
                "foreground": "#ff6666"
            }
        },
        {
            "scope": [
                "markup.inserted",
                "meta.diff.header.to-file",
                "punctuation.definition.inserted"
            ],
            "settings": {
                "background": "#8b000020",
                "foreground": "#8b0000"
            }
        },
        {
            "scope": [
                "markup.changed",
                "punctuation.definition.changed"
            ],
            "settings": {
                "background": "#ff888820",
                "foreground": "#ff8888"
            }
        },
        {
            "scope": [
                "markup.ignored",
                "markup.untracked"
            ],
            "settings": {
                "foreground": "#2a0f0f",
                "background": "#ff6666"
            }
        },
        {
            "scope": "meta.diff.range",
            "settings": {
                "foreground": "#8b0000",
                "fontStyle": "bold"
            }
        },
        {
            "scope": "meta.diff.header",
            "settings": {
                "foreground": "#ff6666"
            }
        },
        {
            "scope": "meta.separator",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#ff6666"
            }
        },
        {
            "scope": "meta.output",
            "settings": {
                "foreground": "#ff6666"
            }
        },
        {
            "scope": [
                "brackethighlighter.tag",
                "brackethighlighter.curly",
                "brackethighlighter.round",
                "brackethighlighter.square",
                "brackethighlighter.angle",
                "brackethighlighter.quote"
            ],
            "settings": {
                "foreground": "#8b0000"
            }
        },
        {
            "scope": "brackethighlighter.unmatched",
            "settings": {
                "foreground": "#ff6666"
            }
        },
        {
            "scope": [
                "constant.other.reference.link",
                "string.other.link"
            ],
            "settings": {
                "foreground": "#b22222",
                "fontStyle": "underline"
            }
        }
    ]
}
