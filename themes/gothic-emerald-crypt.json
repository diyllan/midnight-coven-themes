  "semanticTokenColors": {
    "property": "#40a070",
    "type": {
      "foreground": "#10b060",
      "bold": true
    },
    "class": {
      "foreground": "#30d070",
      "bold": true
    },
    "enum": {
      "foreground": "#60e090",
      "bold": true
    },
    "interface": {
      "foreground": "#10b060",
      "italic": true
    },
    "function": "#30d070",
    "variable.readonly": {
      "foreground": "#50c080",
      "italic": true
    },
    "macro": {
      "foreground": "#20c080",
      "bold": true
    },
    "method": {
      "foreground": "#30d070",
      "bold": true
    },
    "namespace": {
      "foreground": "#50c080",
      "italic": true
    },
    "parameter": {
      "foreground": "#d0e0d8",
      "italic": true
    },
    "property.readonly": {
      "foreground": "#60e090",
      "italic": true
    },
    "constructor": {
      "foreground": "#20c080",
      "bold": true
    },
    "parameter.declaration": {
      "foreground": "#d0e0d8",
      "underline": true
    }
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#507060",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "constant.other.symbol"
      ],
      "settings": {
        "foreground": "#d0c060"
      }
    },
    {
      "name": "Constants and Numbers",
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.character",
        "constant.escape"
      ],
      "settings": {
        "foreground": "#e0d080"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "string constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#d0e0d8"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#40d090",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Functions and Classes",
      "scope": [
        "entity.name.function",
        "entity.name.class",
        "entity.name.type",
        "entity.name.namespace",
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#20c080",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Storage types",
      "scope": [
        "storage.type"
      ],
      "settings": {
        "foreground": "#c050a0",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Types and Support",
      "scope": [
        "support.class",
        "support.type",
        "support.function",
        "support.constant"
      ],
      "settings": {
        "foreground": "#50a0d0"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#40c0c0"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "meta.brace",
        "meta.delimiter"
      ],
      "settings": {
        "foreground": "#80a090"
      }
    },
    {
      "name": "JSON Property Names",
      "scope": [
        "support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#40d090",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Headings",
      "scope": [
        "markup.heading"
      ],
      "settings": {
        "foreground": "#20c080",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Bold",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup Links",
      "scope": [
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#50a0d0"
      }
    },
    {
      "name": "Markup Lists",
      "scope": [
        "markup.list"
      ],
      "settings": {
        "foreground": "#d0c060"
      }
    },
    {
      "name": "Markup Quotes",
      "scope": [
        "markup.quote"
      ],
      "settings": {
        "foreground": "#c050a0",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Inline Documentation",
      "scope": [
        "comment.block.documentation"
      ],
      "settings": {
        "foreground": "#608070",
        "fontStyle": "italic"
      }
    },
    {
      "name": "RegExp",
      "scope": [
        "string.regexp"
      ],
      "settings": {
        "foreground": "#40c0c0"
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "tag.decorator.js",
        "meta.tag.decorator",
        "meta.decorator"
      ],
      "settings": {
        "foreground": "#c050a0",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Template Literals",
      "scope": [
        "string.template",
        "punctuation.definition.string.template"
      ],
      "settings": {
        "foreground": "#d0c060"
      }
    },
    {
      "name": "Special Variables",
      "scope": [
        "variable.language",
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#f07070",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Crystal Facets",
      "scope": [
        "entity.name.section",
        "entity.name.module"
      ],
      "settings": {
        "foreground": "#40e090",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Emerald Glimmers",
      "scope": [
        "constant.other.color",
        "support.constant.color"
      ],
      "settings": {
        "foreground": "#20c080"
      }
    },
    {
      "name": "Mineral Deposits",
      "scope": [
        "markup.table",
        "markup.raw"
      ],
      "settings": {
        "foreground": "#90e080",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Gemstone Reflections",
      "scope": [
        "variable.other.object.property",
        "support.variable.property"
      ],
      "settings": {
        "foreground": "#70d0a0",
        "fontStyle": "italic"
      }
    }
  ]
}
