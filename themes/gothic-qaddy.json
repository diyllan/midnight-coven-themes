  "semanticTokenColors": {
    "enumMember": "#4fc1ff",
    "variable": "#9cdcfe",
    "variable.readonly": "#4fc1ff",
    "property": "#9cdcfe",
    "function": "#dcdcaa",
    "method": "#dcdcaa",
    "parameter": "#9cdcfe",
    "class": "#4ec9b0",
    "interface": "#b8d7a3",
    "type": "#4ec9b0",
    "namespace": "#4ec9b0",
    "macro": "#569cd6",
    "keyword": "#569cd6",
    "comment": "#8b7355",
    "string": "#b5e07a",
    "number": "#b5cea8"
  },
  "tokenColors": [
    {
      "name": "JSON Property Names",
      "scope": [
        "support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#9cdcfe"
      }
    },
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#8b7355",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "constant.other.symbol"
      ],
      "settings": {
        "foreground": "#b5e07a"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": [
        "string.regexp"
      ],
      "settings": {
        "foreground": "#d16969"
      }
    },
    {
      "name": "Numbers",
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#b5cea8"
      }
    },
    {
      "name": "Built-in constants",
      "scope": [
        "constant.language"
      ],
      "settings": {
        "foreground": "#569cd6"
      }
    },
    {
      "name": "Constants",
      "scope": [
        "constant.character",
        "constant.escape"
      ],
      "settings": {
        "foreground": "#4fc1ff"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable"
      ],
      "settings": {
        "foreground": "#9cdcfe"
      }
    },
    {
      "name": "Variables and object properties",
      "scope": [
        "variable.parameter",
        "variable.other"
      ],
      "settings": {
        "foreground": "#9cdcfe"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#569cd6"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#d4d4d4"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function"
      ],
      "settings": {
        "foreground": "#dcdcaa"
      }
    },
    {
      "name": "Classes and Types",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "entity.other.inherited-class",
        "support.class",
        "support.type"
      ],
      "settings": {
        "foreground": "#4ec9b0"
      }
    },
    {
      "name": "Storage types",
      "scope": [
        "storage.type"
      ],
      "settings": {
        "foreground": "#569cd6"
      }
    },
    {
      "name": "Tags and HTML Elements",
      "scope": [
        "entity.name.tag",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#569cd6"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#9cdcfe"
      }
    },
    {
      "name": "CSS Properties",
      "scope": [
        "support.type.property-name.css"
      ],
      "settings": {
        "foreground": "#9cdcfe"
      }
    },
    {
      "name": "CSS Classes and IDs",
      "scope": [
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.id.css"
      ],
      "settings": {
        "foreground": "#d7ba7d"
      }
    },
    {
      "name": "Imports and Modules",
      "scope": [
        "variable.import.parameter.js",
        "variable.other.class.js"
      ],
      "settings": {
        "foreground": "#9cdcfe"
      }
    },
    {
      "name": "Methods",
      "scope": [
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#dcdcaa"
      }
    },
    {
      "name": "Block Level Variables",
      "scope": [
        "meta.block variable.other"
      ],
      "settings": {
        "foreground": "#9cdcfe"
      }
    },
    {
      "name": "Other Variable",
      "scope": [
        "support.other.variable"
      ],
      "settings": {
        "foreground": "#9cdcfe"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation.definition.parameters.begin.bracket.round.scss"
      ],
      "settings": {
        "foreground": "#d4d4d4"
      }
    },
    {
      "name": "Template Strings",
      "scope": [
        "string.quoted.template"
      ],
      "settings": {
        "foreground": "#ce9178"
      }
    },
    {
      "name": "Template String Expressions",
      "scope": [
        "meta.template.expression"
      ],
      "settings": {
        "foreground": "#d4d4d4"
      }
    },
    {
      "name": "YAML Key",
      "scope": [
        "entity.name.tag.yaml"
      ],
      "settings": {
        "foreground": "#9cdcfe"
      }
    },
    {
      "name": "YAML String",
      "scope": [
        "source.yaml string.unquoted"
      ],
      "settings": {
        "foreground": "#d4d4d4"
      }
    }
  ]
}
