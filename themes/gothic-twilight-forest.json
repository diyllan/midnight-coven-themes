  "semanticTokenColors": {
    "property": "#80c0a0",
    "type": {
      "foreground": "#60c0c0",
      "bold": true
    },
    "class": {
      "foreground": "#60d080",
      "bold": true
    },
    "enum": {
      "foreground": "#e0c060",
      "bold": true
    },
    "interface": {
      "foreground": "#60c0c0",
      "italic": true
    },
    "function": "#60d080",
    "variable.readonly": {
      "foreground": "#a0d0b0",
      "italic": true
    },
    "macro": {
      "foreground": "#40b060",
      "bold": true
    },
    "method": {
      "foreground": "#60d080",
      "bold": true
    },
    "namespace": {
      "foreground": "#a0d0b0",
      "italic": true
    },
    "parameter": {
      "foreground": "#d5e5e0",
      "italic": true
    },
    "property.readonly": {
      "foreground": "#80e0a0",
      "italic": true
    },
    "constructor": {
      "foreground": "#40b060",
      "bold": true
    },
    "parameter.declaration": {
      "foreground": "#d5e5e0",
      "underline": true
    }
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#567568",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "constant.other.symbol"
      ],
      "settings": {
        "foreground": "#e0c060"
      }
    },
    {
      "name": "Constants and Numbers",
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.character",
        "constant.escape"
      ],
      "settings": {
        "foreground": "#e0a050"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "string constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#d5e5e0"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#80e0a0",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Functions and Classes",
      "scope": [
        "entity.name.function",
        "entity.name.class",
        "entity.name.type",
        "entity.name.namespace",
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#60d080",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Storage types",
      "scope": [
        "storage.type"
      ],
      "settings": {
        "foreground": "#c070c0",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Types and Support",
      "scope": [
        "support.class",
        "support.type",
        "support.function",
        "support.constant"
      ],
      "settings": {
        "foreground": "#60a0e0"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#60c0c0"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "meta.brace",
        "meta.delimiter"
      ],
      "settings": {
        "foreground": "#90b0a0"
      }
    },
    {
      "name": "JSON Property Names",
      "scope": [
        "support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#80e0a0",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Headings",
      "scope": [
        "markup.heading"
      ],
      "settings": {
        "foreground": "#60d080",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Bold",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup Links",
      "scope": [
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#60a0e0"
      }
    },
    {
      "name": "Markup Lists",
      "scope": [
        "markup.list"
      ],
      "settings": {
        "foreground": "#e0c060"
      }
    },
    {
      "name": "Markup Quotes",
      "scope": [
        "markup.quote"
      ],
      "settings": {
        "foreground": "#c070c0",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Inline Documentation",
      "scope": [
        "comment.block.documentation"
      ],
      "settings": {
        "foreground": "#678577",
        "fontStyle": "italic"
      }
    },
    {
      "name": "RegExp",
      "scope": [
        "string.regexp"
      ],
      "settings": {
        "foreground": "#60c0c0"
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "tag.decorator.js",
        "meta.tag.decorator",
        "meta.decorator"
      ],
      "settings": {
        "foreground": "#c070c0",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Template Literals",
      "scope": [
        "string.template",
        "punctuation.definition.string.template"
      ],
      "settings": {
        "foreground": "#e0c060"
      }
    },
    {
      "name": "Special Variables",
      "scope": [
        "variable.language",
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#f07070",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Forest Path Markers",
      "scope": [
        "entity.name.section",
        "entity.name.module"
      ],
      "settings": {
        "foreground": "#80e0a0",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Twilight Mist",
      "scope": [
        "meta.object-literal.key",
        "variable.object.property"
      ],
      "settings": {
        "foreground": "#a0e0c0",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Ancient Tree Roots",
      "scope": [
        "meta.import",
        "meta.export"
      ],
      "settings": {
        "foreground": "#60a080",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Forest Canopy",
      "scope": [
        "meta.objectliteral",
        "meta.object.member"
      ],
      "settings": {
        "foreground": "#80c0a0"
      }
    },
    {
      "name": "Dappled Sunlight",
      "scope": [
        "keyword.operator.expression",
        "keyword.operator.new"
      ],
      "settings": {
        "foreground": "#e0c060",
        "fontStyle": "bold"
      }
    }
  ]
}
