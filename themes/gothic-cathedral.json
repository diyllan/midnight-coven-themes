  "semanticTokenColors": {
    "property": "#afa195",
    "type": {
      "foreground": "#9a7286",
      "bold": true
    },
    "class": {
      "foreground": "#75535a",
      "bold": true
    },
    "enum": {
      "foreground": "#a89377",
      "bold": true
    },
    "interface": {
      "foreground": "#9a7286",
      "italic": true
    },
    "function": "#75535a",
    "variable.readonly": {
      "foreground": "#7d6d5c",
      "italic": true
    },
    "macro": {
      "foreground": "#c29275",
      "bold": true
    },
    "method": {
      "foreground": "#75535a",
      "bold": true
    },
    "namespace": {
      "foreground": "#7d6d5c",
      "italic": true
    },
    "parameter": {
      "foreground": "#d3c4b0",
      "italic": true
    },
    "property.readonly": {
      "foreground": "#a89377",
      "italic": true
    },
    "constructor": {
      "foreground": "#c29275",
      "bold": true
    },
    "parameter.declaration": {
      "foreground": "#d3c4b0",
      "underline": true
    }
  },
  "tokenColors": [
    {
      "name": "JSON Property Names",
      "scope": [
        "support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#9a7286",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#6d5f5a"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "constant.other.symbol"
      ],
      "settings": {
        "foreground": "#b39362"
      }
    },
    {
      "name": "Constants and Numbers",
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.character",
        "constant.escape"
      ],
      "settings": {
        "foreground": "#c29275"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "variable.parameter",
        "variable.other"
      ],
      "settings": {
        "foreground": "#d3c4b0"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#9a7286",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function"
      ],
      "settings": {
        "foreground": "#b27c85"
      }
    },
    {
      "name": "Classes and Types",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "entity.other.inherited-class",
        "support.class",
        "support.type"
      ],
      "settings": {
        "foreground": "#869362",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Storage types",
      "scope": [
        "storage.type"
      ],
      "settings": {
        "foreground": "#7c8993",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Tags and HTML Elements",
      "scope": [
        "entity.name.tag",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#7c8993"
      }
    },
    {
      "name": "Attributes",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#869362"
      }
    },
    {
      "name": "Property",
      "scope": [
        "variable.other.property",
        "support.variable.property"
      ],
      "settings": {
        "foreground": "#b27c85"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#c29275"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#d14040"
      }
    },
    {
      "name": "Modified",
      "scope": [
        "markup.changed",
        "markup.deleted",
        "markup.inserted"
      ],
      "settings": {
        "foreground": "#b27c85"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "punctuation.definition",
        "punctuation.separator",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#e6deca"
      }
    },
    {
      "name": "Markdown Headings",
      "scope": [
        "markup.heading",
        "entity.name.section"
      ],
      "settings": {
        "foreground": "#b39362",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Link",
      "scope": [
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#7c8993"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "foreground": "#9a7286",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Inline Documentation",
      "scope": [
        "comment.block.documentation"
      ],
      "settings": {
        "foreground": "#857875",
        "fontStyle": "italic"
      }
    },
    {
      "name": "RegExp",
      "scope": [
        "string.regexp"
      ],
      "settings": {
        "foreground": "#7c8993"
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "tag.decorator.js",
        "meta.tag.decorator",
        "meta.decorator"
      ],
      "settings": {
        "foreground": "#9a7286",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Template Literals",
      "scope": [
        "string.template",
        "punctuation.definition.string.template"
      ],
      "settings": {
        "foreground": "#c29275"
      }
    },
    {
      "name": "Special Variables",
      "scope": [
        "variable.language",
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#b27c85",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Cathedral Illumination",
      "scope": [
        "markup.inline.raw",
        "markup.raw.block"
      ],
      "settings": {
        "foreground": "#c29275",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Cathedral Manuscript",
      "scope": [
        "comment.block.documentation.html",
        "comment.block.documentation.js",
        "comment.block.documentation.ts"
      ],
      "settings": {
        "foreground": "#a89377",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Gargoyle Warnings",
      "scope": [
        "entity.name.label",
        "entity.name.section.group-title"
      ],
      "settings": {
        "foreground": "#9a5a67",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Stained Glass Highlights",
      "scope": [
        "constant.other.color",
        "support.constant.color"
      ],
      "settings": {
        "foreground": "#9a8aa5"
      }
    }
  ]
}
