    "tokenColors": [
        {
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#5c4a32"
            }
        },
        {
            "scope": [
                "constant",
                "entity.name.constant",
                "variable.other.constant",
                "variable.language"
            ],
            "settings": {
                "foreground": "#cd853f"
            }
        },
        {
            "scope": [
                "entity",
                "entity.name"
            ],
            "settings": {
                "foreground": "#daa520"
            }
        },
        {
            "scope": "variable.parameter.function",
            "settings": {
                "foreground": "#d4c5a9"
            }
        },
        {
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#8b7355"
            }
        },
        {
            "scope": "keyword",
            "settings": {
                "foreground": "#8b7355",
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "storage",
                "storage.type"
            ],
            "settings": {
                "foreground": "#8b7355",
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "storage.modifier.package",
                "storage.modifier.import",
                "storage.type.java"
            ],
            "settings": {
                "foreground": "#d4c5a9"
            }
        },
        {
            "scope": [
                "string",
                "punctuation.definition.string",
                "string punctuation.section.embedded source"
            ],
            "settings": {
                "foreground": "#a68660"
            }
        },
        {
            "scope": "support",
            "settings": {
                "foreground": "#cd853f"
            }
        },
        {
            "scope": "meta.property-name",
            "settings": {
                "foreground": "#daa520"
            }
        },
        {
            "scope": "variable",
            "settings": {
                "foreground": "#d4c5a9"
            }
        },
        {
            "scope": "variable.other",
            "settings": {
                "foreground": "#d4c5a9"
            }
        },
        {
            "scope": "invalid.broken",
            "settings": {
                "fontStyle": "bold italic underline",
                "foreground": "#cd853f"
            }
        },
        {
            "scope": "invalid.deprecated",
            "settings": {
                "fontStyle": "bold italic underline",
                "foreground": "#cd853f"
            }
