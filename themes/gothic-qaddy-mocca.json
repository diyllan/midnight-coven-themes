    "keyword": "#f5c2e7",
    "comment": "#6e6c7e",
    "string": "#a6e3a1",
    "number": "#f9e2af"
  },
  "tokenColors": [
    {
      "name": "JSON Property Names",
      "scope": [
        "support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#cba6f7"
      }
    },
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#6e6c7e",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "constant.other.symbol"
      ],
      "settings": {
        "foreground": "#a6e3a1"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": [
        "string.regexp"
      ],
      "settings": {
        "foreground": "#f28fad"
      }
    },
    {
      "name": "Numbers",
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#f9e2af"
      }
    },
    {
      "name": "Built-in constants",
      "scope": [
        "constant.language"
      ],
      "settings": {
        "foreground": "#f5c2e7"
      }
    },
    {
      "name": "Constants",
      "scope": [
        "constant.character",
        "constant.escape"
      ],
      "settings": {
        "foreground": "#f5c2e7"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable"
      ],
      "settings": {
        "foreground": "#cba6f7"
      }
    },
    {
      "name": "Variables and object properties",
      "scope": [
        "variable.parameter",
        "variable.other"
      ],
      "settings": {
        "foreground": "#cba6f7"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#f5c2e7"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#f5e0dc"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function"
      ],
      "settings": {
        "foreground": "#96cdfb"
      }
    },
    {
      "name": "Classes and Types",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "entity.other.inherited-class",
        "support.class",
        "support.type"
      ],
      "settings": {
        "foreground": "#a6e3a1"
      }
    },
    {
      "name": "Storage types",
      "scope": [
        "storage.type"
      ],
      "settings": {
        "foreground": "#f5c2e7"
      }
    },
    {
      "name": "Tags and HTML Elements",
      "scope": [
        "entity.name.tag",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#f5c2e7"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#cba6f7"
      }
    },
    {
      "name": "CSS Properties",
      "scope": [
        "support.type.property-name.css"
      ],
      "settings": {
        "foreground": "#cba6f7"
      }
    },
    {
      "name": "CSS Classes and IDs",
      "scope": [
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.id.css"
      ],
      "settings": {
        "foreground": "#f9e2af"
      }
    },
    {
      "name": "Imports and Modules",
      "scope": [
        "variable.import.parameter.js",
        "variable.other.class.js"
      ],
      "settings": {
        "foreground": "#cba6f7"
      }
    },
    {
      "name": "Methods",
      "scope": [
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#96cdfb"
      }
    },
    {
      "name": "Block Level Variables",
      "scope": [
        "meta.block variable.other"
      ],
      "settings": {
        "foreground": "#cba6f7"
      }
    },
    {
      "name": "Other Variable",
      "scope": [
        "support.other.variable"
      ],
      "settings": {
        "foreground": "#cba6f7"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation.definition.parameters.begin.bracket.round.scss"
      ],
      "settings": {
        "foreground": "#f5e0dc"
      }
    },
    {
      "name": "Template Strings",
      "scope": [
        "string.quoted.template"
      ],
      "settings": {
        "foreground": "#a6e3a1"
      }
    },
    {
      "name": "Template String Expressions",
      "scope": [
        "meta.template.expression"
      ],
      "settings": {
        "foreground": "#f5e0dc"
      }
    },
    {
      "name": "YAML Key",
      "scope": [
        "entity.name.tag.yaml"
      ],
      "settings": {
        "foreground": "#cba6f7"
      }
    },
    {
      "name": "YAML String",
      "scope": [
        "source.yaml string.unquoted"
      ],
      "settings": {
        "foreground": "#f5e0dc"
      }
    }
  ]
}
