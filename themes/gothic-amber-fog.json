  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#777070",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "constant.other.symbol"
      ],
      "settings": {
        "foreground": "#e5c875"
      }
    },
    {
      "name": "Constants and Numbers",
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.character",
        "constant.escape"
      ],
      "settings": {
        "foreground": "#e0b86c"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "string constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#e8e1d5"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#d5a84a",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Functions and Classes",
      "scope": [
        "entity.name.function",
        "entity.name.class",
        "entity.name.type",
        "entity.name.namespace",
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#b5c080",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Storage types",
      "scope": [
        "storage.type"
      ],
      "settings": {
        "foreground": "#ad90aa",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Types and Support",
      "scope": [
        "support.class",
        "support.type",
        "support.function",
        "support.constant"
      ],
      "settings": {
        "foreground": "#7a90ad"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#90ada5"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "meta.brace",
        "meta.delimiter"
      ],
      "settings": {
        "foreground": "#9e9a90"
      }
    },
    {
      "name": "JSON Property Names",
      "scope": [
        "support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#d5a84a",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Headings",
      "scope": [
        "markup.heading"
      ],
      "settings": {
        "foreground": "#d5a84a",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Bold",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup Links",
      "scope": [
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#7a90ad"
      }
    },
    {
      "name": "Markup Lists",
      "scope": [
        "markup.list"
      ],
      "settings": {
        "foreground": "#e5c875"
      }
    },
    {
      "name": "Markup Quotes",
      "scope": [
        "markup.quote"
      ],
      "settings": {
        "foreground": "#ad90aa",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Inline Documentation",
      "scope": [
        "comment.block.documentation"
      ],
      "settings": {
        "foreground": "#878080",
        "fontStyle": "italic"
      }
    },
    {
      "name": "RegExp",
      "scope": [
        "string.regexp"
      ],
      "settings": {
        "foreground": "#90ada5"
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "tag.decorator.js",
        "meta.tag.decorator",
        "meta.decorator"
      ],
      "settings": {
        "foreground": "#ad90aa",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Template Literals",
      "scope": [
        "string.template",
        "punctuation.definition.string.template"
      ],
      "settings": {
        "foreground": "#e5c875"
      }
    },
    {
      "name": "Special Variables",
      "scope": [
        "variable.language",
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#d5736a",
        "fontStyle": "italic"
      }
    }
  ]
}
